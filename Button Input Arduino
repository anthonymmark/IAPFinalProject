const int button1 = 2;     // the number of the pushbutton pin
const int button2 = 3;
const int button3 = 4;
const int button4 = 5;
const int button5 = 6;

void setup() {
  Serial.begin(9600); 
  pinMode(button1, INPUT);  
  pinMode(button2, INPUT); 
  pinMode(button3, INPUT); 
  pinMode(button4, INPUT); 
  pinMode(button5, INPUT); 
}

void loop(){
  // read the state of the button1 value:
  
  button1State = digitalRead(button1);
  button2State = digitalRead(button1);
  button3State = digitalRead(button1);
  button4State = digitalRead(button1);
  button5State = digitalRead(button1);
  // check if the pushbutton is pressed.
  // if it is, the buttonState is HIGH:
  if(button1State == HIGH){
    Serial.println('A');
  if(button2State == HIGH){
    Serial.println('B');
  if(button3State == HIGH){
    Serial.println('C');
  if(button4State == HIGH){
    Serial.println('D');
  if(button5State == HIGH){
    Serial.println('E');
  }

}
