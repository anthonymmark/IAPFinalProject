import processing.serial.*;

String task3 = "Test Thursday at 7:30pm";
String task1 = "Practice Today at 5:00pm";
String task2 = "UROP meeting tomorrow at 2:00pm";

void setup() {
  size(displayWidth,displayHeight);
}

void draw() {
  background(255);
  fill(0);
  textSize(56);
  text(task1,150,300);
  text(task2,150,375);
  text(task3,150,450);
}

  
